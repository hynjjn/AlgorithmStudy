#include <iostream>
#include <stack>
#include <vector>

using namespace std;

int arr[100001];
stack<int> a;
vector<char> v;

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);

    int n;
    cin >> n;
    int cur = 1;

    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    
    for (int i = 0; i < n; i++) {
        if (arr[i] > cur) { // 4 > 1, 4 > 2, 4 > 3, 4 > 4
            a.push(cur);
            v.push_back('+');
            cur++;
            cout << "pushed" << '\n';
        } else if (arr[i] == cur) {
            while (!a.empty()) {
                // cout << a.top();
                a.pop();
                v.push_back('-');
            }
        } else if (arr[i] < cur) {
            
        }
    }

    for(int i = 0; i < v.size(); i++) {
        cout << v[i] << '\n';
    }

	return 0;
}